
public class DisplayTree {
	
	public static void main(String[] args){
		String expression = "((1+2)+(3*4))";
		
		char[] x = new char[expression.length()];
        int i;
        int z = 0;
        int a = expression.length();
        while (z < a) {
            x[z] = expression.charAt(z);
            System.out.println(x[z]);
            z++;
            
        }
    }
				
}
	


